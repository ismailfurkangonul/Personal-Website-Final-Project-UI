@{
    Layout = "~/Views/Shared/_EducationLayout.cshtml";

}
<div ng-controller="EducationController">

    <div class="center">
        
        <div ng-repeat="education in EducationList">
            <div class="content">
             <h1>{{education.title}}</h1>   
             <h2>{{education.schoolname1}}</h2>          
             <p>{{education.schoolsubtitle1}}</p>    
             <img src="{{education.image}}" />
             <h2>{{education.schoolname2}}</h2>
             <p>{{education.schoolsubtitle2}}</p>
                <img src="{{education.image1}}" />
             
            </div>
        </div>
        

    </div>
</div>
@section Scripts{
        <script src="/Admin/pages/education.js"></script>
}