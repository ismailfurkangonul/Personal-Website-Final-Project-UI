@{
    Layout = "~/Views/Shared/_LayoutCertificate.cshtml";

}
<div ng-controller="CertificatesController">

    <div class="center">
        
        <div ng-repeat="certificates in CertificatesList">
            <div class="content">
            <h1>{{certificates.title}}</h1>   
            <h2>{{certificates.certificate1}}</h2>
            <h2>{{certificates.certificate2}}</h2>
            <h2>{{certificates.certificate3}}</h2>
            <h2>{{certificates.certificate4}}</h2>
            <h2>{{certificates.certificate5}}</h2>
            <h2>{{certificates.certificate6}}</h2>
            </div>
        </div>

    </div>
</div>
@section Scripts{
        <script src="/Admin/pages/certificates.js"></script>
}